#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aunogarafat/Work/conversation/app/web/node_modules/.pnpm/convex-helpers@0.1.111_@standard-schema+spec@1.1.0_convex@1.31.6_react@19.2.3__hono@4.1_b9cc8d3d3889d96e666378735e6d1ede/node_modules/convex-helpers/node_modules:/Users/aunogarafat/Work/conversation/app/web/node_modules/.pnpm/convex-helpers@0.1.111_@standard-schema+spec@1.1.0_convex@1.31.6_react@19.2.3__hono@4.1_b9cc8d3d3889d96e666378735e6d1ede/node_modules:/Users/aunogarafat/Work/conversation/app/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aunogarafat/Work/conversation/app/web/node_modules/.pnpm/convex-helpers@0.1.111_@standard-schema+spec@1.1.0_convex@1.31.6_react@19.2.3__hono@4.1_b9cc8d3d3889d96e666378735e6d1ede/node_modules/convex-helpers/node_modules:/Users/aunogarafat/Work/conversation/app/web/node_modules/.pnpm/convex-helpers@0.1.111_@standard-schema+spec@1.1.0_convex@1.31.6_react@19.2.3__hono@4.1_b9cc8d3d3889d96e666378735e6d1ede/node_modules:/Users/aunogarafat/Work/conversation/app/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../convex-helpers@0.1.111_@standard-schema+spec@1.1.0_convex@1.31.6_react@19.2.3__hono@4.1_b9cc8d3d3889d96e666378735e6d1ede/node_modules/convex-helpers/bin.cjs" "$@"
else
  exec node  "$basedir/../../../../../../convex-helpers@0.1.111_@standard-schema+spec@1.1.0_convex@1.31.6_react@19.2.3__hono@4.1_b9cc8d3d3889d96e666378735e6d1ede/node_modules/convex-helpers/bin.cjs" "$@"
fi
